<div class="center hero-unit">
  <h1>Idea Farm</h1>
  <div class="row">
  <div class="center">
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.text_field :email, placeholder: "Name" %>
      <br />
      <%= f.password_field :password, placeholder: "Password" %>
      <br />
      <%= f.submit "Sign in", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
</div>
